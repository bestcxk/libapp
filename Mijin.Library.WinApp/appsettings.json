  /// <summary>
  /// 窗口高
  /// </summary>
  "WindowHeight": 500,

  /// <summary>
  /// 窗口宽
  /// </summary>
  "WindowWidth": 500,

  /// <summary>
  /// 显示窗口标题栏
  /// </summary>
  "ShowWindowTitleBar": true,

  /// <summary>
  /// 可否手动调整窗口宽高
  /// </summary>
