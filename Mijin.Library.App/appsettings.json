{
  /*
  ! WindowHeight 和 WindowWidth 都为0 时则直接全屏

  ! 如果打开的URL是本地路径的html文件,请在文件路径加双斜杠
  simple:
    错误的写法：
      "NoSelectOpenUrl": "C:\Users\zy\Desktop\2021Project\test.html"
    正确的写法：
      "NoSelectOpenUrl": "C:\\Users\\zy\\Desktop\\2021Project\\test.html"

  ! 清空缓存的枚举 ，输右边的int 值
    /// <summary>
    /// 不清空
    /// </summary>
    Default = 0,
    /// <summary>
    /// 清空cookie
    /// </summary>
    Cookie = 1,
    /// <summary>
    /// 清空LocalState 
    /// </summary>
    LocalState = 2,
    /// <summary>
    /// 删除webview 下的Default文件 
    /// </summary>
    DefaultCache = 3,
    /// <summary>
    /// 删除整个webview 的Cache文件
    /// </summary>
    AllCache = 4,

  */

  /// <summary>
  /// 当前设备Id
  /// </summary>
  "Id": 11,

  /// <summary>
  /// 后台管理打开的URL
  /// </summary>
  "LibraryManageUrl": "http://localhost:8000/user",

  /// <summary>
  /// 自助借阅打开的URL
  /// </summary>
  "ReaderActionUrl": "http://localhost:8000/terminal",

  /// <summary>
  /// 此字段若有值，则跳过选择窗口直接打开页面窗口
  /// </summary>
  "NoSelectOpenUrl": "",

  /// <summary>
  /// 窗口高
  /// </summary>
  "WindowHeight": 0,

  /// <summary>
  /// 窗口宽
  /// </summary>
  "WindowWidth": 0,

  /// <summary>
  /// 显示窗口标题栏
  /// </summary>
  "ShowWindowTitleBar": true,

  /// <summary>
  /// 可否手动调整窗口宽高
  /// </summary>
  "CanResize": true,
  /// <summary>
  /// 程序退出时清空缓存类型 , 类型查看该文件头部
  /// </summary>
  "OnExitClearWebCache": 0,
  /// <summary>
  /// 门禁Url
  /// </summary>
  "DoorUrls": null,
  "ShowTitleBarBtns": false,
  "CannotClosed": false,

  /// <summary>
  /// 开发者模式
  /// </summary>
  "IsDev": true,

  "Titles": {
    "App": "管理系统",
    "Manager": "后台",
    "Terminal": "借阅"
  }
}